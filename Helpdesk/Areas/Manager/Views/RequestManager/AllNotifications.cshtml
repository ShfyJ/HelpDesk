<script src="~/lib/jquery/dist/jquery.min.js"></script>
<table id="tblNotification" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Title</th>
  
            <th>DateTime</th>
        </tr>
    </thead>
    <tbody>

    </tbody>
</table>

<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            type: "GET",
            dataType: "json",
            url: "/Manager/RequestManager/GetNotificaitons",
            traditional: true,
            contentType: "application/json;charset=utf-8",
            success: function (data)
            
            {
                console.log("Here we are");
                var notifications = data;
                console.log(notifications);
                $.map(notifications, function (notification) {
                    $("#tblNotification tbody").append("tr"
                        + "<td>" + notification.rName + "</td>"
                        + "<td>" + notification.rDateTime + "</td>"
                        + "</tr>"
                    );
                })
            },
            error: function (xhr, status, error) {}
        });
    });
</script>